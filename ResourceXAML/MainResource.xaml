<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:viewmodel="clr-namespace:QuanLyPhongTro.ViewModel"
                    xmlns:view="clr-namespace:QuanLyPhongTro.View"
                    xmlns:window="clr-namespace:QuanLyPhongTro"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Khai báo ViewModel -->
    <viewmodel:DashboardViewModel x:Key="DashboardVM" />
    <viewmodel:MainViewModel x:Key="MainVM" />
    <viewmodel:LoginViewModel x:Key="LoginVM" />
    <viewmodel:RoomViewModel x:Key="RoomVM" />
    <viewmodel:UnitViewModel x:Key="UnitVM" />
    <viewmodel:ServiceViewModel x:Key="ServiceVM" />
    <viewmodel:TenantViewModel x:Key="TenantVM" />
    <viewmodel:MeterReadingViewModel x:Key="MeterReadingVM" />
    <viewmodel:FixViewModel x:Key="FixVM" />
    <viewmodel:ContractViewModel x:Key="ContractVM" />
    <viewmodel:OccupantViewModel x:Key="OccupantVM" />
    <viewmodel:InvoiceViewModel x:Key="InvoiceVM" />
    <viewmodel:PunishViewModel x:Key="PunishVM" />
    <viewmodel:StatisticViewModel x:Key="StatisticVM" />
    <viewmodel:UserViewModel x:Key="UserVM" />

    <!-- Khai báo DataTemplate -->
    <DataTemplate DataType="{x:Type viewmodel:MainViewModel}">
        <view:MainView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:RoomViewModel}">
        <view:RoomView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:ContractViewModel}">
        <view:ContractView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:FixViewModel}">
        <view:FixView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:InvoiceViewModel}">
        <view:InvoiceView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:PunishViewModel}">
        <view:PunishView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:ServiceViewModel}">
        <view:ServiceView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:StatisticViewModel}">
        <view:StatisticView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:TenantViewModel}">
        <view:TenantView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:OccupantViewModel}">
        <view:OccupantView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:UnitViewModel}">
        <view:UnitView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:MeterReadingViewModel}">
        <view:MeterReadingView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:UserViewModel}">
        <view:UserView />
    </DataTemplate>

</ResourceDictionary>
